{
  "name": "@sundaeswap/sdk",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "workspaces": {
    "packages": [
      "./packages/*"
    ],
    "catalog": {
      "@blaze-cardano/core": "0.7.0",
      "@blaze-cardano/sdk": "0.2.43",
      "@blaze-cardano/data": "0.6.4",
      "@sundaeswap/asset": "1.0.11",
      "@sundaeswap/bigint-math": "0.6.3",
      "@sundaeswap/fraction": "1.0.8",
      "@blaze-cardano/emulator": "0.3.34",
      "@types/jest": "^30",
      "@types/node": "^20",
      "@types/bun": "1.3.0",
      "typescript": "^5.7.2"
    }
  },
  "scripts": {
    "lint": "eslint --ignore-pattern '**/dist/**'",
    "lint:fix": "bun run lint --fix",
    "prepare": "husky",
    "typecheck": "sh ./scripts/typecheck.sh",
    "build": "sh ./scripts/build.sh",
    "ci:publish": "sh ./scripts/publish.sh",
    "ci:canary": "sh ./scripts/canary.sh",
    "docs": "sh ./scripts/docs.sh",
    "scan-deps": "depcruise packages --progress"
  },
  "engines": {
    "node": ">=18"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "bun lint:fix --quiet"
    ]
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.2",
    "@semantic-release/git": "^10.0.1",
    "@sundaeswap/babel-preset": "^2.0.15",
    "@sundaeswap/docgen": "^0.1.9",
    "@sundaeswap/eslint-config": "^2.0.15",
    "@sundaeswap/prettier-config": "^2.0.13",
    "@types/bun": "^1.3.0",
    "@types/node": "^18.19.87",
    "concurrently": "^8.2.2",
    "dependency-cruiser": "^17.0.2",
    "glob": "^8.1.0",
    "globals": "^15.15.0",
    "husky": "^9.1.7",
    "jest-fetch-mock": "^3.0.3",
    "lint-staged": "^15.5.1",
    "log-update": "^6.1.0",
    "prettier": "^3.5.3",
    "typedoc": "0.26.7",
    "typedoc-plugin-frontmatter": "1.0.0",
    "typedoc-plugin-markdown": "4.2.7",
    "typescript": "~5.5.4"
  },
  "trustedDependencies": [
    "@parcel/watcher",
    "@swc/core",
    "chacha-native",
    "cpu-features",
    "nx",
    "ssh2",
    "yarn"
  ]
}
