{
  "name": "@sundaeswap/components",
  "version": "1.0.0",
  "description": "Officially supported React components for SundaeSwap protocol actions.",
  "repository": "git@github.com:SundaeSwap-finance/sundae-sdk.git",
  "author": {
    "name": "SundaeSwap Labs, Inc.",
    "url": "https://sundae.fi/"
  },
  "bugs": {
    "url": "https://github.com/SundaeSwap-finance/sundae-sdk/issues"
  },
  "homepage": "https://github.com/SundaeSwap-finance/sundae-sdk#readme",
  "license": "MIT",
  "private": false,
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "src",
    "package.json"
  ],
  "type": "module",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "scripts": {
    "clean": "rm -rf ./dist",
    "typecheck": "tsc --noEmit",
    "compile:esm": "tsc --outDir dist/esm",
    "compile:cjs": "tsc --outDir dist/cjs --module commonjs",
    "compile:types": "tsc --outDir dist/types --emitDeclarationOnly --declarationMap --declaration",
    "compile": "yarn compile:cjs && yarn compile:esm && yarn compile:types",
    "build": "yarn clean && yarn compile",
    "prepare": "yarn build",
    "watch": "yarn compile:cjs && yarn compile:types && yarn compile:esm --watch",
    "docs": "docgen generate --typedoc-options ./typedoc.json",
    "docs:ci": "yarn docs --unsafe"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/user-event": "^14.3.0",
    "@types/jest": "^28.1.3",
    "@types/node": "^18.11.18",
    "husky": "^7.0.0",
    "jest": "^29.3.1",
    "jest-dom": "^4.0.0",
    "lint-staged": "^12.4.1",
    "prettier": "^2.6.2",
    "ts-jest": "^29.0.3",
    "typescript": "^4.6.4",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@types/styled-components": "5.1.34",
    "@types/lodash": "^4.17.7"
  },
  "peerDependencies": {
    "@sundaeswap/asset": "^1.0.6",
    "@sundaeswap/cpp": "^1.0.7",
    "@sundaeswap/core": "^1.2.9",
    "@sundaeswap/fraction": "^1.0.5",
    "lucid-cardano": "^0.10.7",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "styled-components": "^6.1.12"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  }
}
