{
  "$schema": "https://typedoc.org/schema.json",
  "out": "../../docs/typescript/core",
  "entryPoints": [
    "./src/exports/core.ts",
    "./src/exports/testing.ts",
    "./src/exports/utilities.ts"
  ],
  "entryModule": "README.md",
  "hideBreadcrumbs": true,
  "excludeNotDocumented": true,
  "githubPages": false,
  "gitRevision": "main",
  "treatWarningsAsErrors": true,
  "frontmatterCommentTags": [
    "title",
    "layout",
    "nav_excluded",
    "nav_order",
    "child_nav_order",
    "has_toc",
    "grand_parent",
    "parent",
    "has_children"
  ]
}
