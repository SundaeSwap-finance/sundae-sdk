{
  "name": "@sundaeswap/cli",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "author": "SundaeSwap Labs Inc.",
  "license": "UNLICENSED",
  "version": "1.1.0",
  "description": "CLI for the SundaeSwap protocol SDK.",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf ./dist",
    "build:cli": "bun build ./src/index.ts --outdir=./dist/cli --target=node",
    "build": "bun clean && bun build:cli",
    "watch": "bun clean && bun run build --watch"
  },
  "devDependencies": {
    "@sundaeswap/core": "workspace:*",
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@blaze-cardano/sdk": "0.2.31",
    "@inquirer/prompts": "^7.5.0",
    "@sundaeswap/asset": "^1.0.7",
    "@sundaeswap/core": "^2.6.0",
    "asciify-image": "^0.1.10",
    "clipboardy": "^4.0.0"
  },
  "bin": "./dist/cli/index.js"
}