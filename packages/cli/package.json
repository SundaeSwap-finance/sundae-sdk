{
  "name": "@sundaeswap/cli",
  "module": "index.ts",
  "type": "module",
  "author": "SundaeSwap Labs Inc.",
  "license": "UNLICENSED",
  "version": "1.1.3",
  "description": "CLI for the SundaeSwap protocol SDK.",
  "keywords": [
    "cardano",
    "defi",
    "cli",
    "sundaeswap",
    "cryptocurrency"
  ],
  "homepage": "https://github.com/SundaeSwap-finance/sundae-sdk",
  "repository": {
    "type": "git",
    "url": "https://github.com/SundaeSwap-finance/sundae-sdk.git",
    "directory": "packages/cli"
  },
  "bugs": {
    "url": "https://github.com/SundaeSwap-finance/sundae-sdk/issues"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf ./dist",
    "build:cli": "bun build ./src/index.ts --outdir=./dist/cli --target=node",
    "build": "bun clean && bun build:cli",
    "watch": "bun clean && bun run build --watch"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@sundaeswap/core": "^2.6.5",
    "@inquirer/prompts": "^7.5.0",
    "@sundaeswap/asset": "^1.0.11",
    "asciify-image": "^0.1.10",
    "clipboardy": "^4.0.0"
  },
  "peerDependencies": {
    "typescript": "^5.7.2",
    "@blaze-cardano/sdk": "^0.2.31"
  },
  "bin": "./dist/cli/index.js",
  "files": [
    "dist/",
    "README.md"
  ],
  "engines": {
    "node": ">=18.0.0"
  }
}
